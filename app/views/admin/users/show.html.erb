<form class="form-basic">
    
<h2><%= @user.first_name%> <%= @user.last_name%></h2>
<h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900"><%= @user.email%></h5>

<%= link_to "Edit", edit_admin_user_path %>
<%= button_to "Delete", admin_user_path(@user), method: :delete, data: { confirm: "Are you sure?" } %>
<%= link_to "Go back to Dashboard", admin_users_path, class:"d-flex justify-content-center mt-4" %>

</form>
